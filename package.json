{
  "name": "@efrane/vuex-json-api",
  "version": "0.0.18",
  "description": "A Vuex library for interfacing with Json:Api endpoints",
  "license": "MIT",
  "authors": [
    "eFrane <efrane@meanderingsoul.com>"
  ],
  "homepage": "https://vuex-json-api.efrane.com",
  "main": "index.js",
  "scripts": {
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    "test": "yarn unit",
    "tdd": "yarn unit --watch",
    "lint": "eslint --ext .js,.vue src test/unit",
    "build": "yarn lint && yarn test && rimraf ./dist && webpack --config webpack.config.js",
    "vuepress:apidoc": "YARN_SILENT=1 yarn jsdoc2md -f 'src/**/*' --private -l js --name-format --separators --template docs/.vuepress/jsdoctemplate.handlebars > docs/code/README.md",
    "vuepress:dev": "yarn vuepress:apidoc && vuepress dev docs",
    "vuepress:build": "yarn vuepress:apidoc && vuepress build docs"
  },
  "dependencies": {
    "axios": "^0.*",
    "json-api-normalizer": "^0.4",
    "qs": "~6.7.0",
    "vue": "2.*"
  },
  "peerDependencies": {
    "deep-object-diff": "1.*",
    "vuex": "3.*"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.2.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-jest": "^21.0.2",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "chalk": "^2.0.1",
    "css-loader": "^0.28.0",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.0.0",
    "husky": "^2.4.0",
    "jest": "^22.0.4",
    "jest-serializer-vue": "^0.3.0",
    "jsdoc-to-markdown": "^5.0.0",
    "mermaid": "^8.0.0",
    "node-sass": "^4.12.0",
    "ora": "^1.2.0",
    "portfinder": "^1.0.13",
    "semver": "^5.3.0",
    "shelljs": "^0.7.6",
    "vue-jest": "^1.0.2",
    "vue-template-compiler": "^2.5.2",
    "vuepress": "^0.14.11"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  }
}
